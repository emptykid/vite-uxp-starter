{
  "manifestVersion": 4,
  "name": "vite-uxp-panel",
  "version": "1.0.0",
  "host": {
    "app": "PS",
    "minVersion": "23.0.0"
  },
  "main": "index.html",
  "entrypoints": [
    {
      "id": "vite-uxp-panel",
      "label": {
        "default": "vite uxp panel"
      },
      "type": "panel",
      "minimumSize": {
        "width": 320,
        "height": 320
      },
      "maximumSize": {
        "width": 1000,
        "height": 1000
      },
      "preferredDockedSize": {
        "width": 320,
        "height": 570
      },
      "preferredFloatingSize": {
        "width": 320,
        "height": 570
      },
      "icons": [
        {
          "path": "icons/panel_icon_dark.png",
          "width": 23,
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ],
          "height": 23
        },
        {
          "path": "icons/panel_icon_light.png",
          "width": 23,
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "height": 23
        }
      ]
    }
  ],
  "id": "006f6afc",
  "icons": [
    {
      "path": "icons/panel_icon_dark.png",
      "width": 23,
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ],
      "height": 23
    },
    {
      "path": "icons/panel_icon_light.png",
      "width": 23,
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "height": 23
    }
  ],
  "requiredPermissions": {
    "clipboard": "readAndWrite",
    "network": {
      "domains": "all"
    },
    "allowCodeGenerationFromStrings": true
  }
}
